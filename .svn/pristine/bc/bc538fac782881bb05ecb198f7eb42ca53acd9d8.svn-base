//反馈
$(function() {
	$("#feedbackBtn").click(function(){
		feedback();
	});
	
});
//反馈校验
function feedback() {
	if(Validator.validateNull($("#advice").val())){
		$.popAlert("请输入您的反馈意见");
		$("#advice").focus();
		return false;
	}
	
	if(Validator.validateNull($("#feedbackqq").val())){
		$.popAlert("请留下您的联系方式");
		$("#feedbackqq").focus();
		return false;
	}
}
