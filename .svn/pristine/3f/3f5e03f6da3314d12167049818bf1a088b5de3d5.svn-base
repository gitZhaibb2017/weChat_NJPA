@charset "UTF-8";

body{
	margin:0px;
	font-weight: normal;
	background-color: rgb(245,245,245);
}
.li_one{
	margin:3rem;
	font-size:29px;
	font-weight:bold;
	
}
.li_two{
	margin:2rem;
	font-size:20px;
	font-weight:bold;
}
 .span{
   font-size:18px;
 }
  
  .photoDiv {
  	text-align: center;
  	border-bottom:1px solid #eee;
  }
  .photo1{
     width: 24%; 
	border: 3px solid #ffa500 ;
	border-radius:60%; 
	text-align: center;
	margin-top:1%; 
	
  
  }
  .photo {
	width: 50%;
	border: 4px solid #ffa500;
	border-radius:60%;
	margin-top:10px;
} 
.photoInner{
	width:50%;
	position:relative;
	margin:0 auto;
}

.photo_ca{
   width:11%;
   position:absolute;
   left:64%;
   top:79%;
   
}
.lineInfo2 {
	width: 100%;
	height:170%;
	line-height: 170%;
	font-size:25px;  
}
.lineInfo ul li{
	clear:both;
	height:40px;
	line-height: 40px; 
	padding-left:10px;
	font-size:14px;
	padding-left:20px;
}
.lineInfo2 ul li{
	font-size:14px;
	height:40px;
	line-height:40px;
	clear:both;
	border-bottom:1px solid #eee;
	padding-left:20px;
}
.lineInfo2 ul li span:nth-child(1){
	float:left;
}
.lineInfo2 ul li span:nth-child(2){
	float:right;
}
.lineInfo2 ul li span:nth-child(2) img{
	vertical-align:middle;
	width:50%;
}

.lineInfo2 ul li span:nth-child(3){
	float:right; 
	margin-right:10px;
	color:rgb(156,156,156);
}

.lineInfo ul li span:first-child{
	float:left;
}
.lineInfo ul li span:last-child {
	float:right;
	margin-right:4px;
}
.lineInfo ul li span:nth-child(2){
	margin-right: 4px;
	float:right;
}

.lineInfo ul li:nth-child(3),.lineInfo ul li:nth-child(4){
	border-bottom:1px solid #eee;
}
.lineInfo ul li span:nth-child(2) img{
	width:50%;
	vertical-align:middle;
}

#footer{
    width: 100%;
    text-align: center;
    background: rgb(245,245,245);
    height:80px;
 }
 

.integralDetail2 {
	height: 40px;
	line-height: 40px;
	color: rgb(152,152,152);
	background-color: rgb(245,245,245);
	font-size: 14px;
	padding-left: 2rem;
	border-bottom:1px solid rgb(226,226,226);
}

/* pop */


.lineInfo ul li a{
	color:#058eff;
	font-size: 14px;
}

 
 .fmpw li span:nth-child(1){
	 width:20%;
	 display:inline-block;
	 
}
.fmpw li span:nth-child(2){
	 width:70%;
	 display:inline-block;
	 margin-right:10px;
} 

 .fmpw li {
	margin-bottom: 10px;
}

.fmpw #spwd {
	margin-left: 13px;
}
.fmpw #op span:nth-child(2) span {
	width:80%;
	 color:red;
	 text-align:left;
}


.fmpw [type='password']#SurePwd {
	margin-left: -12px;
}

#footer .res {
    width: 90%;
    height: 3rem;
    color: #fff;
    font-size: 1.4rem;
    border: 0;
    font-weight:bold;
    border-radius: 3rem;
    text-align: center;
    background-color: rgb(204,204,204);
    margin-top:20px;
}

/*解绑手机begin*/
.mobileTip {
	height: 4rem;
	line-height: 4rem;
	color: #cdb470;
	font-size: 1.4rem;
	text-indent: 2.66%;
}


  .bindMobile #getMobileCode {
	height: 40px;
    line-height: 40px;
    text-align: center;
    border: none;
    border-left: 2px solid #f5f5f5;
    background: #fff;
    color: #666;
    width: 30%;
    font-size: 12px;
    padding: 0 0;
} 
 
.othUnbind{
	height: 6rem;
	line-height: 6rem;
	font-size: 1.2rem;
	margin-left: 2.66%;
	
}
/*解绑手机end*/

/*绑定手机*/
.bindMobile li{
	height:40px;
	line-height:40px;
	background:#fff;
	margin-bottom:10px;
	font-size:12px;
}
.bindMobile li span:nth-child(1){
	margin-left:10px;
	width:25%;
	display:inline-block;
	
}
.bindMobile li span:nth-child(2) input{
	border:none;
	height:30px;
	line-height:30px;
	width:40%;
	font-size:12px;
}
.bindMobile li button{	
	height:40px;
	line-height:40px;
	text-align:center;
	border:none;
	border-left:2px solid #f5f5f5;	
	background:#fff;
	color:#666;
	width:25%;
	font-size:12px;
	padding:0 0;
}
.bindMobile footer{
	width:100%;
	text-align:center;
}
.bindMobile .subBtn{
	width: 90%;
    height: 3rem;
    color: #fff;
    font-size: 1.4rem;
    border: 0;
    border-radius: 3rem;
    text-align: center;
    background-color: rgb(204,204,204);
    margin-top: 10px;
}


