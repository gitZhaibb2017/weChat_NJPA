var app = angular.module("questionDetail", []);
app.controller("questionDetailCtrl", [ '$scope', '$http', function($scope, $http) {
	$scope.name='dingleilei';
	$.ajax({
		type : 'POST',
		url :"/pawj/app/investigation/investigationDetails.do",
		headers:{'token':'001a19a0-7e86-49d9-ba83-aac8b50a031a'},
		data:{'investigationID':$("#questionId").val()},
		dataType : "JSON",
		success : function(data) { 	
			$scope.questions = data.questions;
			$scope.$apply();
		}
	});
	$scope.show=function(i,obj){
		obj.className = "icon_starL0"+i;
	}
	$scope.getStarCls = function(option){
		if(option.className){
			return option.className;
		}
		return 'icon_starL00';
	}
	
} ]);
