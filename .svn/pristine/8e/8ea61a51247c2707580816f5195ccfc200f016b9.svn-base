package com.paic.pawj.test.service.impl;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.paic.pawj.basic.user.dao.UserMapper;
import com.paic.pawj.test.service.TestService;

/**
 * @author happy
 *
 */
@Service("testService")
public class TestServiceImpl implements TestService {
    
    @Resource
    private UserMapper testDao;

    /* (non-Javadoc)
     * @see com.paic.pawj.test.service.TestService#getUserName(java.lang.String)
     */
    @Override
    public String getUserName(String userId) {
        return testDao.getUsernameByID(userId).getUserName();
    }

}
