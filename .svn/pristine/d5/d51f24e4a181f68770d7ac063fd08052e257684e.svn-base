<html>
<head>
<meta charset="UTF-8" />
<script type="text/javascript" src="/pawj/libs/easyui/jquery.min.js"></script>
<script type="text/javascript"
	src="/pawj/libs/easyui/jquery.easyui.min.js"></script>
<script type="text/javascript"
	src="/pawj/libs/easyui/locale/easyui-lang-zh_CN.js"></script>
<script type="text/javascript" src="/pawj/wjadd/js/wjList.js"></script>
<link rel="stylesheet" type="text/css"
	href="/pawj/libs/easyui/themes/default/easyui.css" />
<link rel="stylesheet" type="text/css"
	href="/pawj/libs/easyui/themes/icon.css" />
<style>
.datagrid-pager .pagination {
	postion: absolute;
	botton: 0px;
}
</style>
</head>
<body onkeydown="entersearch(event);">
	<table id="loginInfoTable" title="问卷列表浏览" border="0" cellspacing="0"
		cellpadding="0" iconCls="icon-edit"
		idField="investigationId" pagination="true" remoteSort="false"
		singleSelect="false" pagePosition="bottom" showFooter="true"
		checkOnSelect="false" split="true" striped="true" rownumbers="true">
		<thead>
			<tr align="center">
				<th field="ck" width="20" checkbox="true" width="20"></th>
				<th field="investigationId" hidden="true"></th>
				<th field="name" formatter='withTitle' width="100">名称</th>
				<th field="questionNum" formatter='withTitle' width="45">题目数</th>
				<th field="userId" formatter='withTitle' width="100">发布者</th>
				<th field="statusDesc" formatter='withTitle' width="45">状态</th>
				<th field="coefficient" formatter='withTitle' width="45">系数</th>
				<th field="planCopy" formatter='withTitle' width="55">预计份数</th>
				<th field="returnCopy" formatter='withTitle' width="55">实际份数</th>
				<th field="totalFee" formatter='withTitle' width="45">总费用</th>
				<th field="actualFee" formatter='withTitle' width="55">实际费用</th>
				<th field="deductFee" formatter='withTitle' width="55">扣除费用</th>
				<th field="actualStartTime" formatter='withTitle' width="120">实际开始时间</th>
				<th field="actualEndTime" formatter='withTitle' width="120">实际结束时间</th>
				<th field="answerFee" formatter='withTitle' width="75">回答钱数(元)</th>
				<th field="opt" formatter='optFormater' width="150">操作</th>
			</tr>
		</thead>
	</table>
	<!-- 添加 -->
	<div id="loginInfoAdd" icon="icon-save"
		style="padding: 5px; width: 500px; height: 400px; top: 100px;">
		<h5 id="loginInfoAdd_message" style="color: red;"></h5>
		<div class="ToolTip_Form" id="table_loginInfoAdd"
			onkeydown="if(event.keyCode==13){loginInfoAdd();}">
			<input type="hidden" id="questionInfoId"></input>
			<table>
				<tr>
					<td><label>问卷名称：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="wjinfo_name" maxlength="20" required="true"></input></td>
				</tr>
				<tr>
					<td><label>发布者：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="wjinfo_auth" maxlength="20" required="true"></input></td>
				</tr>
				<tr>
					<td><label>问卷简介：</label></td>
					<td><textarea class="easyui-validatebox" id="wjinfo_desc"
							rows="5" cols="40"></textarea></td>
				</tr>
				<tr>
					<td><label>预计份数：</label></td>
					<td><input type="number" class="easyui-validatebox"
						id="wjinfo_copies" maxlength="10" required="true"></input></td>
				</tr>
				<tr>
					<td><label>回答回报：</label></td>
					<td><input type="number" class="easyui-validatebox"
						id="wjinfo_answer_fee" maxlength="10" required="true"></input></td>
				</tr>
				<tr>
					<td><label>系数：</label></td>
					<td><input type="number" class="easyui-validatebox"
						id="wjinfo_xishu" maxlength="10" required="true"></input></td>
				</tr>
				<tr>
					<td><label>计划时间：</label></td>
					<td><input class="easyui-datetimebox" id="startTimePlan"
						style="width: 150px"><span>&nbsp;~&nbsp;</span><input
						class="easyui-datetimebox" id="endTimePlan" style="width: 150px"></td>
				</tr>
				<tr>
					<td><label>问卷状态：</label></td>
					<td><select id="wj_status"></select></td>
				</tr>
				<tr>
					<td></td>
					<td><a href="#" class="easyui-linkbutton" icon="icon-ok"
						onclick="loginInfoAdd();">提交</a> <a href="#"
						class="easyui-linkbutton" icon="icon-redo"
						onclick="loginInfoAddReset();">重置</a></td>
				</tr>
			</table>
		</div>
	</div>

	<!-- 编辑 -->
	<div id="loginInfoEdit" icon="icon-save"
		style="padding: 5px; width: 500px; height: 300px;">
		<h5 id="loginInfoEdit_message" style="color: red;"></h5>
		<div class="ToolTip_Form" id="table_loginInfoEdit"
			onkeydown="if(event.keyCode==13){loginInfoEdit();}">
			<input type="hidden" id="loginInfoEdit_loginId"></input>
			<ul>
				<li><label>用户名：</label> <label id="loginInfoEdit_loginCode"></label>
				</li>
				<li><label>密码：</label> <input type="password"
					class="easyui-validatebox" id="loginInfoEdit_password"
					maxlength="20" required="true"></input></li>
				<li><label>重复密码：</label> <input type="password"
					class="easyui-validatebox" id="loginInfoEdit_repassword"
					maxlength="20" required="true"></input></li>
				<li><a href="#" class="easyui-linkbutton" icon="icon-ok"
					onclick="loginInfoEdit();">提交</a></li>
			</ul>
		</div>
	</div>
	<div id="easyui_toolbar" border="false"
		style="border-bottom: 1px solid #ddd; height: 27px; padding: 2px 5px; background: #fafafa;">

		<div>
			<div style="float: left;">
				<a href="#" class="easyui-linkbutton" plain="true" icon="icon-add"
					onclick="openDialog_add();">添加问卷</a>
			</div>

			<div class="datagrid-btn-separator"></div>

			<div style="float: left;">
				<a href="#" class="easyui-linkbutton" plain="true"
					icon="icon-cancel" onclick="batch('delete');">批量删除</a>
			</div>
			<div class="datagrid-btn-separator"></div>
			<div style="float: left;margin-left: 10px;">
				<span>名称:</span> <input id="qsrName"
					style="line-height: 18px; border: 1px solid #ccc; width: 120px"
					maxlength="20"> <span>描述:</span> <input id="qsrDesc"
					style="line-height: 18px; border: 1px solid #ccc; width: 120px"
					maxlength="20"> <span>发布者:</span> <input id="publisher"
					style="line-height: 18px; border: 1px solid #ccc; width: 120px"
					maxlength="20"> <a href="#" class="easyui-linkbutton"
					icon="icon-search" id="searchLink" onclick="doSearch()" plain="true"></a> <a
					href="#" class="easyui-linkbutton" icon="icon-clear"
					onclick="resetForm()" plain="true"></a>
			</div>
			<div class="datagrid-btn-separator" style="margin-left: 10px;"></div>
		</div>
		<!--<div id="tb" style="float: right;">  
                    <input id="ss" class="easyui-searchbox"  
                        searcher="doSearch" menu="#mm" prompt="请输入关键词"  
                        style="width: 250px; vertical-align: middle;"></input>
					<div id="mm" style="width:30px">
						<div data-options="name:'all'">全部</div>
						<div data-options="name:'qsrName'">名称</div>
						<div data-options="name:'qsrDesc'">描述</div>
						<div data-options="name:'label'">标签</div>
						<div data-options="name:'publisher'">发布者</div>
					</div>
                </div>  -->

	</div>
	<div id="questionList"></div>
</body>
</html>