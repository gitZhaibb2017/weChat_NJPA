/* 下拉刷新 */
.refresh {
	text-align: center;
	color: #26B5F5;
	height: 21px;
}
/* 下拉刷新图标 */
.refresh img {
	width: 20px;
	transform: rotate(0deg);
	-moz-transform: rotate(0deg);
	-webkit-transform: rotate18(0deg);
	-o-transform: rotate(180deg);
	animation: godown 0.5s;
	-moz-animation: godown 0.5s;
	-webkit-animation: godown 0.5s;
	-o-animation: godown 0.5s;
}
/* 下拉刷新图标样式改变 */
.refresh img.on {
	transform: rotate(180deg);
	-moz-transform: rotate(180deg);
	-webkit-transform: rotate18(180deg);
	-o-transform: rotate(180deg);
	animation: goup 1s;
	-moz-animation: goup 0.5s;
	-webkit-animation: goup 0.5s;
	-o-animation: goup 0.5s;
}

