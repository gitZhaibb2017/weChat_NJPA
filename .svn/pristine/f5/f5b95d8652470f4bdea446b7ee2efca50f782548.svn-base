/*人脸识别*/
.faceBg {
	width: 100%;
	height: 100%;
	background: #f66 url(../images/faceBg.png) center;
}
.faceBtn {
	margin: 5rem auto;
	text-align: center;
}
.faceBtn button {
	width: 25%;
	height: 80px;
	color: #c00;
	font-size: 3rem;
	background: url(../images/faceBtn.png) repeat-x center;
	border-radius: 10px;
	border: 1px solid #dea303;
	margin: 0 1rem;
	cursor: pointer;
}
.faceMain {
	margin: 15% auto;
	text-align: center;
	position: relative;
