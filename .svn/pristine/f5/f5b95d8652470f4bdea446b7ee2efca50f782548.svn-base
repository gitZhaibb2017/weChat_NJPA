@charset "UTF-8";

:focus {
	outline: none;
	cursor: pointer;
}
body,ol,ul,li,p,h1,h2,h3,h4 {
	margin: 0;
	padding: 0;
	font-weight: normal;
}
html,body {
	color: #666;
	font: normal 62.5% Helvetica, Arial, sans-serif, "微软雅黑";
	background-color: rgb(245,245,245);
	word-break: break-all;
}
img,object {
	border: 0;
	max-width: 100%;
	-ms-interpolation-mode: bicubic;
	cursor: pointer;
}
input {
	outline: none;
}
a,a:hover {
	color: blue;
	text-decoration: none;
	cursor: pointer;
	margin-top:10px;
}
b {
	font-weight: normal;
}
i {
	width: 32px;
    height: 10%;
    background: url(../../images/icon_arrow.png) no-repeat center;
	background-size: 80%;
    vertical-align: middle;
    position: absolute;
	left: 1.5%;
}
ol,ul,li {
	list-style: none;
}
header {
	width: 100%;
	height: 4.5rem;
	line-height: 4.5rem;
	color: #fff;
	font-size: 1.8rem;
	background-color: #fe6446;
	text-align: center;
	vertical-align: middle;
	display: inline-block;
    position: fixed;
}
header i {
	width: 32px;
    height: 100%;
    background: url(../../images/icon_arrow.png) no-repeat center;
	background-size: 80%;
    vertical-align: middle;
    position: absolute;
	left: 1.5%;
}
header b {
	width: 32px;
    height: 100%;
    background: url(../images/icon_ask.png) no-repeat center;
	background-size: 80%;
    vertical-align: middle;
    position: absolute;
	right: 3%;
}
header span {
	width: 70%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}
.bgWhite {
    background: #fff;
}
.bgGray{
	background:rgb(245,245,245);
}

/*头部*/
.top {
	width: 100%;
	color: #fff;
	font-size: 0.6rem;
	background-color: #fd4d4d;
	padding: 0.5rem 0;
	position: relative;
	display: table;
}
.avatar img {
	width: 7.5%;
	min-width: 4rem;
	border-radius: 50%;
	margin-left: 0.5rem;
	display: inline-block;
	float: left;
}
.avatar b {
	color: #f23a1e;
	width: 2.5%;
	font-size: 0.6rem;
	border-radius: 0.6rem;
	padding: 0.1rem 0.5rem;
	background-color: #fbf42e;
	position: relative;
	right: 3%;
	top: 0.5rem;
}
.info {
	margin-top: 1.5rem;
	float: left;
}
.info i {
	padding: 0.25rem;
	border-radius: 0.6rem;
	background-color: #ff6262;
}
.info span {
	color: #fbf42e;
}
.sign {
	color: #4c0202;
	line-height: 1.5rem;
	border-radius: 10rem;
	background-color: #fff6ca;
	padding: 0.25rem 0.5rem;
	margin-right: 1rem;
	float: right;
}
.sign img {
	width: 30%;
	position: relative;
	top: 0.25rem;
}
.sign b {
	font-size: 1.2rem;
	font-weight: 700;
}
.sign a {
	color: #4c0202;
}
.banner {
	width: 100%;
	display: block;
}
	
/*头部/*/

/*导航*/
.nav {
	color: #666;
	font-size: 1.6rem;
	width: 100%;
	background-color: #fff;
	border-bottom: 1px solid #ddd;
	box-shadow: 0px 1px 3px #eee;
	text-align: center;
	display: table;
}
.nav li {
	width: 33.33%;
	text-align: center;
	padding: 1rem 0;
	float: left;
	cursor: pointer;
	display: inline-block;
}
.nav img {
	width: 50%;
}
.nav a i {
	margin-top: 0.5rem;
	width: 100%;
	display: block;
}
.navChoose {
	color: #f60;
}
/*导航/*/

/*问卷*/
.hot_title, .interest_title  {
	color: #333;
	font-size: 1.6rem;
	font-weight: 700;
	text-indent: 1rem;
	padding: 1rem 0;
	background-color: #e8f0f3;
	border-bottom: 1px solid #cfe5ed;
}
.hot_content {
	color: #2e78a7;
	font-size: 1.4rem;
	text-indent: 1rem;
	line-height: 2;
	background-color: #fff;
	padding: 0.5rem 0;
}
.hot_content a {
	color: #2e78a7;
	width: 70%;
	overflow: hidden;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
}
.hot_content span {
	width: 20%;
	height: 1rem;
	line-height: 2;
	background-color: #e9e9e9;
	border-radius: 2rem;
	margin-right: 1rem;
	float: right;
	display: inline-block;
	position: relative;
	top: -1.7rem;
}
.hot_content label {
	height: 1rem;
	border-radius: 2rem;
	background-color: #36be00;
	float: right;
	display: inline-block;
	float: left;
}

.interest_content {
	color: #5b6c7b;
	text-indent: 2.4rem;
	font-size: 1.2rem;
	line-height: 1.5;
	background-color: #fff;
	padding: 1rem;
	display: block;
}
.interest_content a {
	color: #5b6c7b;
}
/*问卷/*/

/*底部*/
footer{
	color: #cdcdcd;
    width: 100%;
	font-size: 1.2rem;
	background-color: rgb(245,245,245);
	padding: 0.25rem 0;
	margin-top: 0.5rem;
	
	position: fixed;
    bottom: 0;
    height:4.5rem;
    line-height:4.5rem;
}
.footer {
	color: #919da2;
    width: 100%;
	font-size: 1.2rem;
	background-color: #fff;
	padding: 0.25rem 0;
	margin-top: 0.5rem;
	border-top: 1px solid #d5dfe2;
	position: fixed;
    bottom: 0;
}
.footer li {
	width: 24%;
	text-align: center;
	display: inline-block;
}
.footer img {
	width: 30%;
}
.footer span {
	width: 4rem;
	height: 4rem;
	background-color: #fff;
	display: block;
}
.footer a {
	color: #919da2;
	display: block;
}
.footer b {
	display: block;
}
/*底部/*/


/*底部隐藏*/
.footer_hidden {
	color: #919da2;
    width: 100%;
	font-size: 1.2rem;
	background-color: #fff;
	padding: 0.25rem 0;
	margin-top: 0.5rem;
	border-top: 1px solid #d5dfe2;
	position: relative;
    bottom: 0;
	visibility: hidden;
}

.footer_hidden li {
	width: 19%;
	text-align: center;
	display: inline-block;
}
.footer_hidden img {
	width: 30%;
}
.footer_hidden span {
	width: 4rem;
	height: 4rem;
	background-color: #fff;
	display: block;
}
.footer_hidden a {
	color: #919da2;
	display: block;
}
.footer_hidden b {
	display: block;
}
/*底部隐藏/*/

/*登录*/
.contain{
	text-align:center;
	width:80%;
	margin:0 auto;
}
.contain .login{
	margin:50px 0 20px 0;
}
.contain .number input,.psword input,.repsword input,.contain .password input{
	border:1px solid #ccc;
	padding:10px 0;
	margin:10px 0;
	border-radius:25px;
	color:#666;
	text-align: center;
	font: normal 62.5% Helvetica, Arial, sans-serif, "微软雅黑";
	font-size:18px;
}
.contain .password input{
	width: 65%;
}
.contain .number input {
	width:100%;
}
.contain .psword input {
	width:100%;
}
.contain .repsword input {
	width:100%;
}
.contain .loginbtn,.contain .getcodebtn{
	background:#ffb71d;
	border:none;
	color:#fff;
	border-radius:25px;
	padding:10px 0;
}
.contain .getcodebtn{
	width: 32%;
}
.contain .loginbtn{
	width:100%;
	margin:20px 0 0 0;
	font-size:20px;
	font-weight:bold;
}
.loginBg {
	background-color: #fe6446;
}
.login img {
	width: 60%;
	padding: 2rem 0;
}
.login_tips {
	width: 60%;
	color: #fff;
	font-size: 1.6rem;
	background-color: rgba(0,0,0,0.5);
	border-radius: 5px;
	padding: 10px 0;
	line-height: 1.5;
	position: fixed;
	top: 50%;
	left: 20%;
	text-align: center;
}
.loginOther {
	text-align: center;
	margin: 1.5rem 0 10rem 0;
}
.loginOther li {
	width: 75%;
	display: inline-block;
}
.loginOther a {
	width: 30%;
	display: inline-block;
}
.loginOther img {
	width: 50%;
	opacity: 0.5;psword
}
.pswd_option a {
	color:white;
	float:right;
	font-size: 15px;
}
/*登录/*/

/*个人中心*/
.person	{
	width: 100%;
	height: 4.2rem;
	vertical-align: middle;
	color: #fff;
	background-color: #fe6446;
	text-align: center;
	padding: 5% 0;
	display: table;
}
.person li {
	display: inline-block;
	float: left;
}

.personL, .personM, .personR {
	width: 33.33%;
}
.personL img, .personR img {
	width: 15%;
	border: 2px solid #ff9393;
	border-radius: 50%;
	text-align: center;
	background-color: #fe7e66;
	padding: 8%;
	margin-top: 30%;
}
.personM {
	position: relative;
}
.personM img {
	width: 80%;
	border: 0.4rem solid #eccc9b;
	border-radius: 50%;
}
.personM i {
	width: 30%;
	min-width:3rem;
	height: 1.5rem;
	line-height: 1.5rem;
	white-space: nowrap;
	color: #b25a1f;
	background: -webkit-gradient(linear, left top, left bottom, from(#ff0), to(#ffd800));
	border-radius: 0.75rem;
	display: inline-block;
	position: absolute;
	top: 10%;
	right: 0;
}
.personM p {
	font-size: 1.6rem;
	margin-top: 5%;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.personContent {
	width: 100%;
	background-color: #fff;
	text-align: center;
	border-collapse: collapse;
}
.personContent tr {
	border-bottom: 1px solid #eee;
}
.personContent tr td:first-child {
	border-right: 1px solid #eee;
}
.personContent td {
	padding: 3% 0;
}
.personContent span {
	display: inline-block;
	cursor: pointer;
}
.personContent label {
	width: 35%;
	display: inline-block;
	text-align: left;
	cursor: pointer;
}
.personContent img {
	width: 50%;
}
.personContent b {
	font-size: 1.6rem;
	line-height: 2;
	display: block;
}
.personContent i {
	color: #999;
}

.personList {
	width: 100%;
	background-color: #fff;
	border-bottom: 1px solid #eee;
	margin-top: 1rem;
	display: table;
}
.mt0 {
	margin-top: 0;
}
.personList li {
	border-bottom: 1px solid #eee;
}
.personList span, .personList label {
	font-size: 1.4rem;
	vertical-align: middle;
	display: inline-block;
}
.personList span {
	width: 90%;
	text-indent: 5%;
}
.personList label {
	width: 10%;
	text-align: center;
}
.personList img {
	width: 30%;
	padding: 10% 0;
}
/*个人中心/*/


/*共享调查*/
.tip {
	height: 3rem;
	line-height: 3rem;
	color: #cdb470;
	background-color: #fbfbee;
	font-size: 1.4rem;
	text-indent: 2.66%;
}
.shareContent {
	width: 100%;
	background-color: #fff;
	border-top: 1px solid #f5f5f5;
	border-bottom: 1px solid #f5f5f5;
	margin-top: 1rem;
	display: table;
}
.shareContent div {
	margin: 1rem;
	overflow: auto;
}
.shareContent h1 {
	width: 80%;
	color: #2e78a7;
	line-height: 1.5;
	font-size: 1.4rem;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	float: left;
}
.shareContent b {
	width: 20%;
	color: #ff3c00;
	font-size: 1.4rem;
	line-height: 1.5;
	text-align: right;
	float: right;
}
.shareContent ul {
	width: 80%;
	float: left;
}
.shareContent li {
	color: #999;
	padding: 0.25rem 0.5rem;
	border-radius: 1rem;
	border: 1px solid #ccc;
	margin-bottom: 0.5rem;
	margin-right: 1rem;
	display: inline-block;
}
.shareContent label {
	width: 20%;
	text-align: right;
	float: right;
}
.shareContent button {
	width: 4rem;
	color: #fff;
	background: -webkit-gradient(linear, left top, left bottom, from(#ffa800), to(#ff6e02));
	padding: 0.25rem 0.5rem;
	border-radius: 1rem;
	border: 1px solid #fc0;
	display: inline-block;
}
/*共享调查/*/

/*积分*/
.integralInfo {
	width: 100%;
	height: 6rem;
	background-color: #fff;
	display: table;
}
.integralInfo span, .integralInfo label, .integralInfo b {
	display: inline-block;
}
.integralInfo span {
	width: 12%;
	margin: 0 1rem;
}
.integralInfo img {
	max-height: 4rem;
	border-radius: 50%;
	border: 1px solid #eccc9b;
	vertical-align: middle;
}
.integralInfo label {
	width: 10%;
	height: 100%;
	background: url(../images/icon_integral02.png) no-repeat center;
	background-size: 80%;
	vertical-align: middle;
}
.integralInfo b {
	font-size: 1.6rem;
}
.integralInfo i {                                          
	color: #f60;
}
.integralDetail {
	height: 3rem;
	line-height: 3rem;
	color: #999;
	background-color: #f5f5f5;
	font-size: 1.4rem;
	text-indent: 1rem;
}
.integralContent {
	background-color: #fff;
	border-collapse: collapse;
}
.integralContent tr {
	height: 4rem;
	font-size: 1.4rem;
	border-collapse: collapse;
	border-bottom: 1px solid #ccc;
}

.integralContent td:nth-child(1) {
	width: 7rem;
	color: #333;
	text-indent: 1rem;
}
.integralContent td:nth-child(2) {
	width: 8rem;
	min-width: 6rem;
	color: #f60;
	text-indent: 1rem;
	background: url(../images/icon_tipOn.png) no-repeat left center;
	background-size: 80%;
}
.integralContent td:nth-child(3) {
	color: #999;
	text-align: right;
	position: relative;
	right: 1rem;
}
/*积分/*/

/*匿名评价*/
.evaluteTitle {
	margin: 4rem 0 4rem 33%;
}
.evaluteTitle h1 {
	font-size: 2rem;
	line-height: 3;
}
.evaluteTitle h2 {
	font-size: 1.4rem;
	line-height: 2;
}
.evaluteTitle span {
	color: #999;
}
.star {
	width: 252px;
	height: 240px;
	background: url(../images/star.png) no-repeat center;
}
.star01, .star02, .star03, .star04, .star05 {
	height: 40px;
	margin: 0 auto;
}
.star01 {
	background-position: 0 0;
}
.star02 {
	background-position: 0 -40px;
}
.star03 {
	background-position: 0 -80px;
}
.star04 {
	background-position: 0 -120px;
}
.star05 {
	background-position: 0 -160px;
}
.icon_starL00, .icon_starL01, .icon_starL02, .icon_starL03, .icon_starL04, .icon_starL05 {
	width: 120px;
	height: 20px;
	display: inline-block;
	position: relative;
	top: 0.3rem;
}
.icon_starL00 {
	background: url(../images/icon_starL00.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL01 {
	background: url(../images/icon_starL01.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL02 {
	background: url(../images/icon_starL02.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL03 {
	background: url(../images/icon_starL03.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL04 {
	background: url(../images/icon_starL04.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL05 {
	background: url(../images/icon_starL05.png) no-repeat left center;
	background-size: 90%;
}
.evaluteMark h1 {
	font-size: 1.4rem;
	color: #333;
	border-top: 1px solid #eee;
	padding: 1rem;
}
.evaluteMark li {
	color: #999;
	border: 1px solid #ddd;
	padding: 0.5rem;
	display: inline-block;
	border-radius: 2rem;
	margin: 0 0 0.5rem 1rem;
	cursor: pointer;
}
.evaluteMarkOn {
	color: #f90 !important;
	border: 1px solid #f90 !important;
}
.evaluteMark div {
	padding: 1rem;
}
.evaluteMark textarea {
	width: 98%;
	height: 4.5rem;
	line-height: 1.5;
	border: 1px solid #ccc;
	border-radius: 0.5rem;
	padding-left: 0.5rem;
}
.evaluteMarkBtn {
	padding: 0 1rem 1rem 1rem;
}
.evaluteMarkBtn button {
	width: 100%;
	height: 3rem;
	color: #fff;
	font-size: 1.4rem;
	border: 0;
	border-radius: 3rem;
	text-align: center;
	background-color: #ffb71d;
}
/*匿名评价/*/

