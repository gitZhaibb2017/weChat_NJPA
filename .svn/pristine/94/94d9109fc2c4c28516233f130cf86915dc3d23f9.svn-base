<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.pawj.test.dao.UserMapper">
	<select id="getUsernameByID" parameterType="String" resultType="com.paic.pawj.test.dao.User">
		select user_name userName from test_user where user_id = #{userID}
	</select>
</mapper>