package com.paic.pawj.auth.service.impl;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.paic.pawj.auth.dao.Admin;
import com.paic.pawj.auth.dao.AdminMapper;
import com.paic.pawj.auth.service.AdminService;

@Service
public class AdminServiceImpl implements AdminService {

	@Resource
	private AdminMapper adminDao;
	
	@Override
	public Admin getAdmin(String userId, String pdd) {
		Admin admin = new Admin();
		admin.setUserId(userId);
		admin.setPdd(pdd);
		return adminDao.getAdmin(admin);
	}

}
