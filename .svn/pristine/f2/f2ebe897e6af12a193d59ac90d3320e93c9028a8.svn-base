@charset "UTF-8";

:focus {
	outline: none;
	cursor: pointer;
}
body,ol,ul,li,p,h1,h2,h3,h4 {
	margin: 0;
	padding: 0;
	font-weight: normal;
}
html,body {
	color: #666;
	font: normal 62.5% Helvetica, Arial, sans-serif, "微软雅黑";
	background-color: #dfe7eb;
	word-break: break-all;
}
img,object {
	border: 0;
	max-width: 100%;
	-ms-interpolation-mode: bicubic;
	cursor: pointer;
}
input {
	outline: none;
}
a,a:hover {
	color: #333;
	text-decoration: none;
	cursor: pointer;
}
b {
	font-weight: normal;
}
i {
	font-style: normal;
}
ol,ul,li {
	list-style: none;
}
header {
	width: 100%;
	height: 4.5rem;
	line-height: 4.5rem;
	color: #fff;
	font-size: 1.8rem;
	background-color: #fe6446;
	text-align: center;
	vertical-align: middle;
	display: inline-block;
    position: relative;
}
header i {
	width: 32px;
    height: 100%;
    background: url(../images/icon_arrow.png) no-repeat center;
	background-size: 80%;
    vertical-align: middle;
    position: absolute;
	left: 1.5%;
}
header b {
	width: 32px;
    height: 100%;
    background: url(../images/icon_ask.png) no-repeat center;
	background-size: 80%;
    vertical-align: middle;
    position: absolute;
	right: 3%;
}
header span {
	width: 70%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}
.bgWhite {
    background: #fff;
}

/*头部*/
.top {
	width: 100%;
	color: #fff;
	font-size: 0.6rem;
	background-color: #fd4d4d;
	padding: 0.5rem 0;
	position: fixed;
	display: table;
	z-index: 10;
}
.avatar img {
	width: 7.5%;
	min-width: 4rem;
	border-radius: 50%;
	margin-left: 0.5rem;
	display: inline-block;
	float: left;
}
.avatar b {
	color: #f23a1e;
	width: 2.5%;
	font-size: 0.6rem;
	border-radius: 0.6rem;
	padding: 0.1rem 0.5rem;
	background-color: #fbf42e;
	position: relative;
	right: 3%;
	top: 0.5rem;
}
.info {
	margin-top: 1.5rem;
	float: left;
}
.info i {
	padding: 0.25rem;
	border-radius: 0.6rem;
	background-color: #ff6262;
}
.info span {
	color: #fbf42e;
}
.show {
	height: 2rem;
    width: 10%;
    margin: 0 auto;
    background-color: #fd4d4d;
    float: left;
    opacity: 0;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}
.sign {
	color: #4c0202;
	line-height: 1.5rem;
	border-radius: 10rem;
	background-color: #fff6ca;
	padding: 0.25rem 0.5rem;
	margin-right: 1rem;
	float: right;
}
.sign img {
	width: 30%;
	position: relative;
	top: 0.25rem;
}
.sign b {
	font-size: 1.2rem;
	font-weight: 700;
}
.sign a {
	color: #4c0202;
}
.banner {
                width: 100%;
                display: inline-block;
	            margin-top: 60px;
            }
            .banner .swipe {
                overflow: hidden;
                position: relative;
            }
            .banner .swipe-wrap {
                overflow: hidden;
                position: relative;
                list-style: none;
            }
            .banner .swipe-wrap li {
                float: left;
                position: relative;
            }
            .banner img {
                width: 100%;
                vertical-align: middle;
            }
            .banner .slide-trigger {
                position: absolute;
                left: 0;
                bottom: 0;
                width: 100%;
                z-index: 10;
                display: -webkit-box;
                display: -moz-box;
                display: -ms-flexbox;
                display: -webkit-flex;
                display: flex;
                -webkit-box-pack: center;
                -moz-box-pack: center;
                -ms-flex-pack: center;
                -webkit-justify-content: center;
                justify-content: center;
                list-style: none;
            }
            .banner .slide-trigger li {
                width: 10px;
                height: 10px;
                background: #FFF;
                margin: 5px;
                -webkit-border-radius: 50%;
                -moz-border-radius: 50%;
                -ms-border-radius: 50%;
                -o-border-radius: 50%;
                border-radius: 50%;
            }
            .banner .slide-trigger .cur {
                background: #2fc7c9;
            }
	
/* 提示层 */
.alert_layer{
	display:none;
	background-image: url("../images/layer_2.png");
	background-size: 100% 100%;
	position: fixed;
	top:300px;
	text-align: center;
	color:#fff;
	left: 25%;
    width: 50%;
    line-height: 40px;
    font-size: 13px;
}

/*导航*/
.nav {
	color: #666;
	font-size: 1.6rem;
	width: 100%;
	background-color: #fff;
	border-bottom: 1px solid #ddd;
	box-shadow: 0px 1px 3px #eee;
	text-align: center;
	display: table;
}
.nav li {
	width: 33.33%;
	text-align: center;
	padding: 1rem 0;
	float: left;
	cursor: pointer;
	display: inline-block;
}
.nav img {
	width: 50%;
}
.nav a i {
	margin-top: 0.5rem;
	width: 100%;
	display: block;
}
.navChoose {
	color: #f60;
}
/*导航/*/

/*问卷*/
.hot_title, .interest_title  {
	color: #333;
	font-size: 1.6rem;
	font-weight: 700;
	text-indent: 1rem;
	padding: 1rem 0;
	background-color: #e8f0f3;
	border-bottom: 1px solid #cfe5ed;
}
.hot_content {
	width: 100%;
	color: #2e78a7;
	font-size: 1.4rem;
	text-indent: 1rem;
	line-height: 2;
	background-color: #fff;
	padding: 0.5rem 0;
	display: table;
}
.hot_content a {
	color: #2e78a7;
	width: 60%;
	overflow: hidden;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
	float: left;
}
.hot_content span {
	width: 10%;
	height: 1rem;
	line-height: 2;
	background-color: #e9e9e9;
	border-radius: 2rem;
	margin-right: 1rem;
	float: left;
	position: relative;
    top: 0.7rem;
}
.hot_content label {
	height: 1rem;
	border-radius: 2rem;
	background-color: #36be00;
	float: left;
}
.hot_content b {
	width: 25%;
    height: 1rem;
    line-height: 2;
}

.interest_content {
	color: #5b6c7b;
	text-indent: 2.4rem;
	font-size: 1.2rem;
	line-height: 1.5;
	background-color: #fff;
	padding: 1rem;
	display: block;
}
.interest_content a {
	color: #5b6c7b;
}
/*问卷/*/

/*底部*/
.footer {
	color: #919da2;
    width: 100%;
	font-size: 1.2rem;
	background-color: #fff;
	padding: 0.25rem 0;
	margin-top: 0.5rem;
	border-top: 1px solid #d5dfe2;
	position: fixed;
    bottom: 0;
}
.footer li {
	width: 24%;
	text-align: center;
	display: inline-block;
}
.footer img {
	width: 30%;
}
.footer span {
	width: 4rem;
	height: 4rem;
	background-color: #fff;
	display: block;
}
.footer a {
	color: #919da2;
	display: block;
}
.footer b {
	display: block;
}
/*底部/*/


/*底部隐藏*/
.footer_hidden {
	color: #919da2;
    width: 100%;
	font-size: 1.2rem;
	background-color: #fff;
	padding: 0.25rem 0;
	margin-top: 0.5rem;
	border-top: 1px solid #d5dfe2;
	position: relative;
    bottom: 0;
	visibility: hidden;
}

.footer_hidden li {
	width: 19%;
	text-align: center;
	display: inline-block;
}
.footer_hidden img {
	width: 30%;
}
.footer_hidden span {
	width: 4rem;
	height: 4rem;
	background-color: #fff;
	display: block;
}
.footer_hidden a {
	color: #919da2;
	display: block;
}
.footer_hidden b {
	display: block;
}
/*底部隐藏/*/

/*登录*/
.login img {
	width: 80%;
}
.loginBg {
	height: 100%;
	background-color: #fe5040;
}
.login {
	color: #fff;
	font: normal 62.5% Helvetica, Arial, sans-serif, "微软雅黑";
	text-align: center;
	padding: 5rem 0;
}
.login input, .login button {
	width: 75%;
	height: 4rem;
	color: #fff;
	font-size: 1.6rem;
	border: 0;
	border: 1px solid #fff;
	border-radius: 2rem;
	background: transparent;
	margin-top: 2rem;
	text-align: center;
}
.login input {
	opacity: 0.8;
}
input::-webkit-input-placeholder {
	color: #fff;
	font-size: 1.6rem;
}
.login button {
	color: #fff;
	background-color: #ffb71d;
	border: 0;
	font-size: 1.6rem;
}
.login span {
	width: 75%;
	height: 3rem;
    line-height: 3;
	color: #fff;
	font-size: 1.4rem;
	text-align: right;
	display: inline-block;
	opacity: 0.8;
}
.login span a, .login span a:hover {
	color: #fff;
	margin-left: 1.5rem;
}
.login label {
	width: 75%;
	height: 1rem;
	border-top: 2px solid #fe6e3e;
	text-align: center;
	margin-top: 5rem;
	display: inline-block;
}
.login i {
	width: 20%;
	height: 2rem;
	line-height: 1.5rem;
	color: #ffe7dd;
	font-size: 1.4rem;
	background-color: #fe5040;
	text-align: center;
	display: inline-block;
	position: relative;
	top: -0.8rem;
}
.loginOther {
	text-align: center;
	margin: 1.5rem 0 10rem 0;
}
.loginOther li {
	width: 75%;
	display: inline-block;
}
.loginOther a {
	width: 30%;
	display: inline-block;
}
.loginOther img {
	width: 50%;
	opacity: 0.5;
}
/*登录/*/

/*个人中心*/
.person	{
	width: 100%;
	height: 4.2rem;
	vertical-align: middle;
	color: #fff;
	background-color: #fe6446;
	text-align: center;
	padding: 5% 0;
	display: table;
}
.person li {
	display: inline-block;
	float: left;
}

.personL, .personM, .personR {
	width: 33.33%;
}
.personL img, .personR img {
	width: 15%;
	border: 2px solid #ff9393;
	border-radius: 50%;
	text-align: center;
	background-color: #fe7e66;
	padding: 8%;
	margin-top: 30%;
}
.personM {
	position: relative;
}
.personM img {
	width: 80%;
	border: 0.4rem solid #eccc9b;
	border-radius: 50%;
}
.personM i {
	width: 30%;
	min-width:3rem;
	height: 1.5rem;
	line-height: 1.5rem;
	white-space: nowrap;
	color: #b25a1f;
	background: -webkit-gradient(linear, left top, left bottom, from(#ff0), to(#ffd800));
	border-radius: 0.75rem;
	display: inline-block;
	position: absolute;
	top: 10%;
	right: 0;
}
.personM p {
	font-size: 1.6rem;
	margin-top: 5%;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.personContent {
	width: 100%;
	background-color: #fff;
	text-align: center;
	border-collapse: collapse;
}
.personContent tr {
	border-bottom: 1px solid #eee;
}
.personContent tr td:first-child {
	border-right: 1px solid #eee;
}
.personContent td {
	padding: 3% 0;
}
.personContent span {
	display: inline-block;
	cursor: pointer;
}
.personContent label {
	width: 35%;
	display: inline-block;
	text-align: left;
	cursor: pointer;
}
.personContent img {
	width: 50%;
}
.personContent b {
	font-size: 1.6rem;
	line-height: 2;
	display: block;
}
.personContent i {
	color: #999;
}

.personList {
	width: 100%;
	background-color: #fff;
	border-bottom: 1px solid #eee;
	margin-top: 1rem;
	display: table;
}
.mt0 {
	margin-top: 0;
}
.personList li {
	border-bottom: 1px solid #eee;
}
.personList span, .personList label {
	font-size: 1.4rem;
	vertical-align: middle;
	display: inline-block;
}
.personList span {
	width: 90%;
	text-indent: 5%;
}
.personList label {
	width: 10%;
	text-align: center;
}
.personList img {
	width: 30%;
	padding: 10% 0;
}
/*个人中心/*/


/*共享调查*/
.tip {
	height: 3rem;
	line-height: 3rem;
	color: #cdb470;
	background-color: #fbfbee;
	font-size: 1.4rem;
	text-indent: 2.66%;
}
.shareContent {
	width: 100%;
	background-color: #fff;
	border-top: 1px solid #f5f5f5;
	border-bottom: 1px solid #f5f5f5;
	margin-top: 1rem;
	display: table;
}
.shareContent div {
	margin: 1rem;
	overflow: auto;
}
.shareContent h1 {
	width: 80%;
	color: #2e78a7;
	line-height: 1.5;
	font-size: 1.4rem;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	float: left;
}
.shareContent b {
	width: 20%;
	color: #ff3c00;
	font-size: 1.4rem;
	line-height: 1.5;
	text-align: right;
	float: right;
}
.shareContent ul {
	width: 80%;
	float: left;
}
.shareContent li {
	color: #999;
	padding: 0.25rem 0.5rem;
	border-radius: 1rem;
	border: 1px solid #ccc;
	margin-bottom: 0.5rem;
	margin-right: 1rem;
	display: inline-block;
}
.shareContent label {
	width: 20%;
	text-align: right;
	float: right;
}
.shareContent button {
	width: 4rem;
	color: #fff;
	background: -webkit-gradient(linear, left top, left bottom, from(#ffa800), to(#ff6e02));
	padding: 0.25rem 0.5rem;
	border-radius: 1rem;
	border: 1px solid #fc0;
	display: inline-block;
}
/*共享调查/*/

/*积分*/
.integralInfo {
	width: 100%;
	height: 6rem;
	background-color: #fff;
	display: table;
}
.integralInfo span, .integralInfo label, .integralInfo b {
	display: inline-block;
}
.integralInfo span {
	width: 12%;
	margin: 0 1rem;
}
.integralInfo img {
	max-height: 4rem;
	border-radius: 50%;
	border: 1px solid #eccc9b;
	vertical-align: middle;
}
.integralInfo label {
	width: 10%;
	height: 100%;
	background: url(../images/icon_integral02.png) no-repeat center;
	background-size: 80%;
	vertical-align: middle;
}
.integralInfo b {
	font-size: 1.6rem;
}
.integralInfo i {                                          
	color: #f60;
}
.integralDetail {
	height: 3rem;
	line-height: 3rem;
	color: #999;
	background-color: #f5f5f5;
	font-size: 1.4rem;
	text-indent: 1rem;
}
.integralContent {
	background-color: #fff;
	border-collapse: collapse;
}
.integralContent tr {
	height: 4rem;
	font-size: 1.4rem;
	border-collapse: collapse;
	border-bottom: 1px solid #ccc;
}

.integralContent td:nth-child(1) {
	width: 7rem;
	color: #333;
	text-indent: 1rem;
}
.integralContent td:nth-child(2) {
	width: 8rem;
	min-width: 6rem;
	color: #f60;
	text-indent: 1rem;
	background: url(../images/icon_tipOn.png) no-repeat left center;
	background-size: 80%;
}
.integralContent td:nth-child(3) {
	color: #999;
	text-align: right;
	position: relative;
	right: 1rem;
}
/*积分/*/

/*匿名评价*/
.evaluteTitle {
	margin: 4rem 0 4rem 33%;
}
.evaluteTitle h1 {
	font-size: 2rem;
	line-height: 3;
}
.evaluteTitle h2 {
	font-size: 1.4rem;
	line-height: 2;
}
.evaluteTitle span {
	color: #999;
}
.star {
	width: 252px;
	height: 240px;
	background: url(../images/star.png) no-repeat center;
}
.star01, .star02, .star03, .star04, .star05 {
	height: 40px;
	margin: 0 auto;
}
.star01 {
	background-position: 0 0;
}
.star02 {
	background-position: 0 -40px;
}
.star03 {
	background-position: 0 -80px;
}
.star04 {
	background-position: 0 -120px;
}
.star05 {
	background-position: 0 -160px;
}
.icon_starL00, .icon_starL01, .icon_starL02, .icon_starL03, .icon_starL04, .icon_starL05 {
	width: 120px;
	height: 20px;
	display: inline-block;
	position: relative;
	top: 0.3rem;
}
.icon_starL00 {
	background: url(../images/icon_starL00.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL01 {
	background: url(../images/icon_starL01.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL02 {
	background: url(../images/icon_starL02.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL03 {
	background: url(../images/icon_starL03.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL04 {
	background: url(../images/icon_starL04.png) no-repeat left center;
	background-size: 90%;
}
.icon_starL05 {
	background: url(../images/icon_starL05.png) no-repeat left center;
	background-size: 90%;
}
.evaluteMark h1 {
	font-size: 1.4rem;
	color: #333;
	border-top: 1px solid #eee;
	padding: 1rem;
}
.evaluteMark li {
	color: #999;
	border: 1px solid #ddd;
	padding: 0.5rem;
	display: inline-block;
	border-radius: 2rem;
	margin: 0 0 0.5rem 1rem;
	cursor: pointer;
}
.evaluteMarkOn {
	color: #f90 !important;
	border: 1px solid #f90 !important;
}
.evaluteMark div {
	padding: 1rem;
}
.evaluteMark textarea {
	width: 98%;
	height: 4.5rem;
	line-height: 1.5;
	border: 1px solid #ccc;
	border-radius: 0.5rem;
	padding-left: 0.5rem;
}
.evaluteMarkBtn {
	padding: 0 1rem 1rem 1rem;
}
.evaluteMarkBtn button {
	width: 100%;
	height: 3rem;
	color: #fff;
	font-size: 1.4rem;
	border: 0;
	border-radius: 3rem;
	text-align: center;
	background-color: #ffb71d;
}
/*匿名评价/*/

/*调查内容*/
.QAProgress {
	width: 100%;
	padding: 1rem 0;
	border-bottom: 1px solid #ddd;
	display: table;
}
.QAProgress span {
	width: 60%;
	background-color: #e9e9e9;
	border-radius: 2rem;
	margin: 0 1rem;
	display: inline-block;
	float: left;
}
.QAProgress label {
	height: 1.4rem;
	border-radius: 2rem;
	background-color: #36be00;
	display: inline-block;
	float: left;
}
.QAProgress b {
	width: 30%;
	font-size: 1.4rem;
	display: inline-block;
	float: left;
}
.QAProgress i {
	color: #f60;
}
.QAContent {
	width: 100%;
	border-bottom: 1px solid #ddd;
	display: table;
}
.QAContent h1 {
	color: #333;
	line-height: 1.5;
	font-size: 1.4rem;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	margin: 1rem 0 0 1rem;
}
.QAContent li {
	color: #666;
	line-height: 1.5;
	padding: 0 0.5rem;
	margin: 0.5rem;
	cursor: pointer;
}
.QAContent img {
	max-width: 200px;
	display: block;
}
.QAContent textarea {
	width: 98%;
	height: 4.5rem;
	line-height: 1.5;
	border: 1px solid #ccc;
	border-radius: 0.5rem;
	padding-left: 0.5rem;
}
.QABtn {
	padding: 1rem;
}
.QABtn button {
	width: 100%;
	height: 3rem;
	color: #fff;
	font-size: 1.4rem;
	border: 0;
	border-radius: 3rem;
	text-align: center;
	background-color: #ffb71d;
}
/*调查内容/*/

/*答题*/
.problemTitle {
	color: #333;
	font-size: 1.4rem;
	background-color: #f5f5f5;
	border-bottom: 1px solid #ddd;
	padding: 1rem;
}
.problemContent {
	padding: 0.5rem 0;
}
.problemContent li {
	color: #2e78a7;
	font-size: 1.4rem;
	text-indent: 1rem;
	background-color: #fff;
	padding: 0.6rem 0;
}
.problemContent a {
	color: #2e78a7;
	overflow: hidden;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
}
/*答题/*/

/*人脸识别*/
.faceBg {
	width: 100%;
	height: 100%;
	background: #f66 url(../images/faceBg.png) center;
}
.faceBtn {
	margin: 5rem auto;
	text-align: center;
}
.faceBtn button {
	width: 25%;
	height: 80px;
	color: #c00;
	font-size: 3rem;
	background: url(../images/faceBtn.png) repeat-x center;
	border-radius: 10px;
	border: 1px solid #dea303;
	margin: 0 1rem;
	cursor: pointer;
}
.faceMain {
	margin: 15% auto;
	text-align: center;
	position: relative;
}
.faceMain img {
	width: 50%;
}
.facePerson {
	position: relative;
	z-index: 1;
}
.facePoint {
	position: absolute;
	z-index: 3;
	bottom: 3%;
	left: 26.3%;
}
.faceBorder {
	position: absolute;
	z-index: 2;
	top: 0;
	left: 0;
}
.faceScan {
	position: absolute;
	z-index: 4;
	top: 0;
	left: 0;
}
.avatarList {
	width: 100%;
	background-color: rgba(255,255,255,0.5);
	padding: 5% 0;
	display: table;
}
.avatarList img {
	width: 25%;
	margin: 0 4%;
	float: left;
}
.avatarBorder {
	border: 1px solid #a4d836;
}
.avatarList div {
	display: inline-block;
}
.logo {
	width: 80%;
	text-align: center;
	margin: 0 auto;
	padding-top: 5rem;
}
