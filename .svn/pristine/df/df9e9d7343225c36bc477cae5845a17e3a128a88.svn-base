<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript" src="/pawj/libs/easyui/jquery.min.js"></script>
<script type="text/javascript"
	src="/pawj/libs/easyui/jquery.easyui.min.js"></script>
<script type="text/javascript"
	src="/pawj/libs/easyui/locale/easyui-lang-zh_CN.js"></script>
<script type="text/javascript" src="/pawj/prize/js/prizeList.js"></script>
<link rel="stylesheet" type="text/css"
	href="/pawj/libs/easyui/themes/default/easyui.css" />
<link rel="stylesheet" type="text/css"
	href="/pawj/libs/easyui/themes/icon.css" />
<style>
.datagrid-pager .pagination {
	postion: absolute;
	botton: 0px;
}
</style>


</head>
<body class="easyui-layout" fit="true">
<div region="center" border="false" style="overflow: true;">
	<div id="toolbar" class="datagrid-toolbar" style="height:auto;display:true;">
	<a id="qry_btn" href="#" class="easyui-linkbutton"
	 data-options="iconCls:'icon-add'" onclick="addPrize();" >添加奖项</a>
	</div>
	<table id="prizeQuery" title="问卷列表浏览" border="0" cellspacing="0"
		cellpadding="0" iconCls="icon-edit"
		idField="investigationId" pagination="true" remoteSort="false"
		singleSelect="false" pagePosition="bottom" showFooter="true"
		checkOnSelect="false" split="true" striped="true" rownumbers="true">
		
	</table>
	<!-- 添加 -->
	<div id="addPrize" class="easyui-dialog" data-options="title:'奖项添加',toolbar:'#dtb',modal:true" style="width:530px;height:300px;padding:10px" class="dicTable" closed=true >
		
			<table>
				 <tr>
					<td><label>奖项编号：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="prizeId" maxlength="20" required="true" ></input></td>
				</tr>
				<tr>
					<td><label>奖项名称：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="prizeName" maxlength="20" required="true"></input></td>
				</tr>
				<tr>
					<td><label>开始时间：</label></td>
					<td><input class="easyui-datetimebox" id="startTime"
						style="width: 150px"><span>&nbsp;~&nbsp;</span>
						<label>结束时间:</label>
						<input
						class="easyui-datetimebox" id="endTime" style="width: 150px"></td>
				</tr>
				<tr>
					<td><label>奖项数量：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="voteNum" maxlength="20" ></input></td>
				</tr>
				<tr>
					<td><label>获奖人数量：</label></td>
					<td><input class="easyui-validatebox" id="winnerNum"
						></input></td>
				</tr>
				<tr>
					<td><label>问卷状态：</label></td>
					<td><select id="status" class="easyui-combobox" style="width:80px;" panelHeight="90">
							<option value="-1">请选择</option>
							<option value="1">正常</option>
							<option value="2">已完成</option>
							<option value="3">弃用</option>
						</select></td>
				</tr>
				<tr>
					<td><label>级别:</label></td>
					<td><input type="text" class="easyui-validatebox" 
					id = "level" maxlength="10"/></td>
				</tr>
				<tr>
					<td></td>
					<td>
						<a href="#" class="easyui-linkbutton" icon="icon-ok"
						onclick="prizeAdd();">提交</a> <a href="#"
						class="easyui-linkbutton" icon="icon-redo"
						onclick="prizeReset();">重置</a></td>
					</td>
				</tr>
			</table>
		
	</div>
	<!--修改  -->
	<div id="updatePrize" class="easyui-dialog" data-options="title:'奖项编辑',toolbar:'#dtb',modal:true" style="width:530px;height:300px;padding:10px" class="dicTable" closed=true >
		<form id="updatePrize"  method="post">
			<table>
				<tr>
					<td><label>奖项编号：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="updatePrizeId" maxlength="20" readonly="readonly" disabled="disabled" >
						</input></td>
				</tr>
				<tr>
					<td><label>奖项名称：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="updatePrizeName" maxlength="20" ></input></td>
				</tr>
				<tr>
					<td><label>开始时间：</label></td>
					<td><input class="easyui-datetimebox" id="updateStartTime"
						style="width: 150px"><span>&nbsp;~&nbsp;</span>
						<label>结束时间:</label>
						<input
						class="easyui-datetimebox" id="updateEndTime" style="width: 150px"></td>
				</tr>
				<tr>
					<td><label>奖项数量：</label></td>
					<td><input type="text" class="easyui-validatebox"
						id="updateVoteNum" maxlength="20" ></input></td>
				</tr>
				<tr>
					<td><label>获奖人数量：</label></td>
					<td><input class="easyui-validatebox" id="updateWinnerNum"
						></input></td>
				</tr>
				<tr>
					<td><label>问卷状态：</label></td>
					<td><select id="updateStatus" class="easyui-combobox" style="width:80px;" panelHeight="70">
							<option value="1">正常</option>
							<option value="2">已完成</option>
							<option value="3">弃用</option>
						</select></td>
				</tr>
				<tr>
					<td><label>级别:</label></td>
					<td><input type="text" class="easyui-validatebox" 
					id = "updateLevel" maxlength="10"/></td>
				</tr>
				<tr>
					<td></td>
					<td><a href="#" class="easyui-linkbutton" icon="icon-ok"
						onclick="updatePrizeAdd();">提交</a> <a href="#"
						class="easyui-linkbutton" icon="icon-redo"
						onclick="prizeClose();">关闭</a></td>
				</tr>
			</table>
		</form>
	</div>
</div>
</body>
</html>