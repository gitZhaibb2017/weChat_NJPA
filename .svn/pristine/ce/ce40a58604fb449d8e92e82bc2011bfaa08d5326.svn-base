<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.pawj.basic.version.dao.VersionMapper">
	<!-- namespace的值 指向所映射的 dao -->
	<resultMap id="versionInfo" type="com.paic.pawj.basic.version.dao.VersionInfo">
		<result column="version_no" property="version" />
		<result column="url" property="url" />
	</resultMap>
	<!--查询用户信息 -->
	<select id="queryVersionInfo" resultMap="versionInfo">
		select pvi.version_no,pvi.url from  pawj_version_info pvi
	</select>
</mapper>