<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.paic.pawj.common.dao.CommonMapper">
	<select id="getDictionaryDetial" parameterType="java.lang.String" 
		resultType="com.paic.pawj.common.dao.DictionaryDetailDAO">
		SELECT pbdd.dic_code dicCode,pbdd.dic_name dicName,pbdd.dic_value dicValue,pbdd.dic_desc dicDesc
		  FROM pawj_basic_dictionary pbd,pawj_basic_dictionary_detail pbdd
		where pbd.dic_id=pbdd.dic_id
		  and pbdd.status='0'
		  and pbd.dic_code=#{code}
	</select>
	
</mapper>
