<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.pawj.wechat.dao.WechatUserMapper">
	
	<select id="getUserByOpenid" resultType="com.paic.pawj.wechat.dao.WechatUser" parameterType="String">
		SELECT user_id userID,user_name userName
		FROM pawj_basic_user p
		where wechat_id=#{openid};
	</select>
</mapper>