package com.paic.pawj.basic.account.service.impl;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.paic.pawj.basic.account.dao.AccountQuery;
import com.paic.pawj.basic.account.dao.AccountQueryMapper;
import com.paic.pawj.basic.account.service.AccountQueryService;

@Service
public class AccountQueryServiceImpl implements AccountQueryService {

	@Resource
	AccountQueryMapper accountQueryDAO;
	
	@Override
	public AccountQuery queryAccountChangesByIvtg(String userID, String investigationID) {
		AccountQuery account = new AccountQuery();
		account.setUserID(userID);
		account.setSourceID(investigationID);
		return accountQueryDAO.queryAccountChangesByIvtg(account);
	}

}
