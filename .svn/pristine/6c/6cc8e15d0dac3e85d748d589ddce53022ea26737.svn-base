package com.paic.pawj.basic.version.service.impl;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;
import org.springframework.stereotype.Service;

import com.paic.pawj.basic.version.dao.VersionInfo;
import com.paic.pawj.basic.version.dao.VersionMapper;
import com.paic.pawj.basic.version.service.VersionInfoService;

/**
 * 版本信息操作
 * 
 * @author Admin
 *
 */
@Service("versionInfoService")
public class VersionInfoServiceImpl implements VersionInfoService {
public static final Log LOG = LogFactory.getLog(VersionInfoServiceImpl.class);
	
	@Resource
	private VersionMapper versionMapper;
	/**
	 * 版本信息查询
	 */
	@Override
	public List<Map<String, Object>> queryVersionInfo() {
		return versionMapper.queryVersionInfo();
	}

}
